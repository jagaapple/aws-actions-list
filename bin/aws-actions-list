#!/usr/bin/env node
const commander = require("commander");
const program = require("../src");
const version = require("../package.json").version;

// When sub command does not exist, show help.
if (process.argv.length === 2) process.argv.push('--help')

commander.version(version, "-v, --version");

commander
  .command("get")
  .description("get and output specified AWS actions")
  .action(program.interactiveExecutor.executeInteractively);

commander.parse(process.argv);
